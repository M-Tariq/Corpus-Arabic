<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.com.corpus.controllers.AddCoupletController"
            prefHeight="200.0" prefWidth="600.0">
    <headerText >
        Add New Couplet.
    </headerText>
    <content>
        <GridPane hgap="10" vgap="10" alignment="CENTER">
            <Label  text="Line 1: " GridPane.rowIndex="1" GridPane.columnIndex="0"/>
            <TextField fx:id="coupletline1"  promptText="Type Line 1 of couplet" minWidth="150" GridPane.rowIndex="1" GridPane.columnIndex="1" />
            <Label  text="Line 2: " GridPane.rowIndex="2" GridPane.columnIndex="0"/>
            <TextField fx:id="coupletline2"  promptText="Type Line 2 of couplet" minWidth="100" GridPane.rowIndex="2" GridPane.columnIndex="1" />

        </GridPane>

    </content>
</DialogPane>
